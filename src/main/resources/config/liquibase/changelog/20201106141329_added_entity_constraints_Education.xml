<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Education.
    -->
    <changeSet id="20201106141329-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="education"
                                 constraintName="fk_education_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="university_id"
                                 baseTableName="education"
                                 constraintName="fk_education_university_id"
                                 referencedColumnNames="id"
                                 referencedTableName="university"/>

        <addForeignKeyConstraint baseColumnNames="educ_type_id"
                                 baseTableName="education"
                                 constraintName="fk_education_educ_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="educ_type"/>

    </changeSet>
</databaseChangeLog>
