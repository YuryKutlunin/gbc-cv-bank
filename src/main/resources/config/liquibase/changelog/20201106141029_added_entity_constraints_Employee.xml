<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Employee.
    -->
    <changeSet id="20201106141029-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="resource_pool_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee_resource_pool_id"
                                 referencedColumnNames="id"
                                 referencedTableName="resource_pool"/>

        <addForeignKeyConstraint baseColumnNames="job_title_id"
                                 baseTableName="employee"
                                 constraintName="fk_employee_job_title_id"
                                 referencedColumnNames="id"
                                 referencedTableName="job_title"/>

    </changeSet>
</databaseChangeLog>
